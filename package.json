{
  "name": "default-avatar-helper",
  "version": "2.0.65",
  "description": "A helper for creating default avatars.",
  "main": "dist/index.js",
  "module": "dist/default-avatar-helper.esm.js",
  "jsnext:main": "dist/default-avatar-helper.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean compile bundle",
    "bundle": "rollup -c",
    "check:format": "prettier --list-different \"**/*.{js,ts}\"",
    "check:types": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:compiled": "shx rm -rf compiled",
    "clean:coverage": "shx rm -rf coverage",
    "clean:dist": "shx rm -rf dist",
    "compile": "tsc -p tsconfig.base.json",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "lint": "eslint .",
    "postbundle": "shx rm -rf compiled && shx cp ./index.js ./dist/index.js",
    "release": "tanem-scripts release",
    "test": "run-s check:* lint build test:*",
    "test:cjs": "jest --config ./scripts/jest/config.cjs.js",
    "test:cjsprod": "cross-env NODE_ENV=production jest --config ./scripts/jest/config.cjs.js",
    "test:esm": "jest --config ./scripts/jest/config.esm.js",
    "test:src": "jest --config ./scripts/jest/config.src.js",
    "test:umd": "jest --config ./scripts/jest/config.umd.js",
    "test:umdprod": "jest --config ./scripts/jest/config.umdprod.js"
  },
  "repository": "github:tanem/default-avatar-helper",
  "keywords": [
    "avatar",
    "avatar-generator",
    "avatar-placeholder",
    "avatars",
    "default-avatar",
    "helper",
    "javascript",
    "typescript"
  ],
  "author": "Tane Morgan (https://github.com/tanem)",
  "license": "MIT",
  "bugs": "https://github.com/tanem/default-avatar-helper/issues",
  "homepage": "https://github.com/tanem/default-avatar-helper",
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@types/jest": "27.0.3",
    "@types/lodash": "4.14.177",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "babel-core": "6.26.3",
    "babel-plugin-lodash": "3.3.4",
    "cross-env": "7.0.3",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "rollup": "2.60.2",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "shx": "0.3.3",
    "tanem-scripts": "6.0.35",
    "ts-jest": "27.0.7",
    "typescript": "4.5.2"
  }
}
