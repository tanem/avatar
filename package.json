{
  "name": "default-avatar-helper",
  "version": "2.0.30",
  "description": "A helper for creating default avatars.",
  "main": "dist/index.js",
  "module": "dist/default-avatar-helper.esm.js",
  "jsnext:main": "dist/default-avatar-helper.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean compile bundle",
    "bundle": "rollup -c",
    "check:format": "prettier --list-different \"**/*.{js,ts}\"",
    "check:types": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:compiled": "del compiled",
    "clean:coverage": "del coverage",
    "clean:dist": "del dist",
    "compile": "tsc -p tsconfig.base.json",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "lint": "eslint .",
    "postbundle": "del compiled && cp-cli ./index.js ./dist/index.js",
    "release": "tanem-scripts release",
    "test": "run-s check:* lint build test:*",
    "test:cjs": "jest --config ./scripts/jest/config.cjs.js",
    "test:cjsprod": "cross-env NODE_ENV=production jest --config ./scripts/jest/config.cjs.js",
    "test:esm": "jest --config ./scripts/jest/config.esm.js",
    "test:src": "jest --config ./scripts/jest/config.src.js",
    "test:umd": "jest --config ./scripts/jest/config.umd.js",
    "test:umdprod": "jest --config ./scripts/jest/config.umdprod.js"
  },
  "repository": "github:tanem/default-avatar-helper",
  "keywords": [
    "avatar",
    "avatar-generator",
    "avatar-placeholder",
    "avatars",
    "default-avatar",
    "helper",
    "javascript",
    "typescript"
  ],
  "author": "Tane Morgan (https://github.com/tanem)",
  "license": "MIT",
  "bugs": "https://github.com/tanem/default-avatar-helper/issues",
  "homepage": "https://github.com/tanem/default-avatar-helper",
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/plugin-transform-runtime": "7.12.15",
    "@babel/preset-env": "7.12.16",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "babel-core": "6.26.3",
    "babel-plugin-lodash": "3.3.4",
    "cp-cli": "2.0.0",
    "cross-env": "7.0.3",
    "del-cli": "3.0.1",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "rollup": "2.39.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "tanem-scripts": "6.0.7",
    "ts-jest": "26.5.1",
    "typescript": "4.1.5"
  }
}
