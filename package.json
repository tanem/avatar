{
  "name": "@tanem/avatar",
  "version": "0.1.0",
  "description": "Generate default avatar details.",
  "main": "dist/index.js",
  "module": "dist/avatar.esm.js",
  "jsnext:main": "dist/avatar.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "authors": "tanem-scripts authors > AUTHORS",
    "build": "run-s clean compile bundle",
    "bundle": "rollup -c",
    "changelog": "tanem-scripts changelog -f v$npm_package_version > CHANGELOG.md",
    "check:format": "prettier --list-different \"**/*.{js,ts}\"",
    "check:types": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:compiled": "del compiled",
    "clean:coverage": "del coverage",
    "clean:dist": "del dist",
    "compile": "tsc -p tsconfig.base.json",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "lint": "eslint .",
    "postbundle": "del compiled && cp-cli ./index.js ./dist/index.js",
    "postversion": "git push && git push --tags && npm publish --access=public",
    "release": "npm version -m 'Release v%s'",
    "test": "run-s check:* lint build test:*",
    "test:cjs": "jest --config ./scripts/jest/config.cjs.js",
    "test:cjsprod": "cross-env NODE_ENV=production jest --config ./scripts/jest/config.cjs.js",
    "test:es": "jest --config ./scripts/jest/config.es.js",
    "test:src": "jest --config ./scripts/jest/config.src.js",
    "test:umd": "jest --config ./scripts/jest/config.umd.js",
    "test:umdprod": "jest --config ./scripts/jest/config.umdprod.js",
    "version": "run-s test changelog authors && git add ."
  },
  "repository": "github:tanem/avatar",
  "keywords": [
    "avatar",
    "avatar-generator",
    "avatar-placeholder",
    "avatars",
    "javascript",
    "typescript"
  ],
  "author": "Tane Morgan (https://github.com/tanem)",
  "license": "MIT",
  "bugs": "https://github.com/tanem/avatar/issues",
  "homepage": "https://github.com/tanem/avatar",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.144",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "babel-core": "6.26.3",
    "babel-plugin-lodash": "3.3.4",
    "codecov": "3.6.1",
    "cp-cli": "2.0.0",
    "cross-env": "6.0.0",
    "del-cli": "3.0.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "24.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rollup": "1.21.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-size-snapshot": "0.10.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "5.1.2",
    "tanem-scripts": "2.0.11",
    "ts-jest": "24.1.0",
    "typescript": "3.6.3"
  }
}
